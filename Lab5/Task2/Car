package com.example.transport.model;

public class Car extends Vehicle {
    private int doors;

    public Car(String brand, int year, int doors) {
        super(brand, year);
        this.doors = doors;
    }

    @Override
    public void showInfo() {
        System.out.println("Car: " + getBrand() + ", year: " + getYear() +
                ", doors: " + doors);
    }
}
