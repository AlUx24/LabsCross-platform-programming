public class LeapYearChecker {
    public static void main(String[] args) {
        int year = 2000; 

        boolean isLeap = false;

        if (year % 4 == 0) {
            if (year % 100 == 0) {
                if (year % 400 == 0) {
                    isLeap = true;
                } else {
                    isLeap = false;
                }
            } else {
                isLeap = true;
            }
        } else {
            isLeap = false;
        }

        if (isLeap) {
            System.out.println("**Високосний**");
        } else {
            System.out.println("**Звичайний**");
        }
    }
}
